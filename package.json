{
	"name": "graycat-sql-formatter",
	"displayName": "GrayCat SQL Formatter",
	"description": "A VSCode formatter extension for SQL scripts.",
	"publisher": "AdamRybak",
	"author": "Adam Rybak <adamrybak85@gmail.com>",
	"repository": {
		"url": "https://github.com/adamrybak/graycat-sql-formatter"
	},
	"version": "0.0.3",
	"license": "MIT",
	"preview": true,
	"main": "./dist/extension.js",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:sql"
	],
	"contributes": {
		"languages": [
			{
				"id": "sql",
				"extensions": [
					".sql",
					".dsql"
				],
				"aliases": [
					"SQL",
					"sql"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/node": "^13.11.0",
		"@types/request": "^2.48.5",
		"@types/vscode": "^1.47.0",
		"request": "^2.88.2",
		"ts-loader": "^8.0.1",
		"typescript": "^3.8.3",
		"webpack-cli": "^3.3.12",
		"webpack": "^4.43.0"
	}
}