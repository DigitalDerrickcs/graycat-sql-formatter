{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"group": "build",
			"label": "Build Debug",
			"script": "webpack",
			"problemMatcher": "$tsc-watch"
		},
		{
			"type": "npm",
			"group": "build",
			"label": "Watch Debug",
			"script": "webpack-dev",
			"problemMatcher": "$tsc-watch"
		},
		{
			"type": "shell",
			"group": "build",
			"label": "Build Package",
			"command": "vsce package",
			"problemMatcher": "$tsc-watch"
		},
		{
			"type": "shell",
			"group": "build",
			"label": "Build Publish",
			"command": "vsce publish",
			"problemMatcher": "$tsc-watch"
		},
	]
}